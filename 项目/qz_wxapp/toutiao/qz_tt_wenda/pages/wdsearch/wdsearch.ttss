@import "../../styles/public.ttss";
@import "../../templates/list-temp/list-temp.ttss";

.search-box{
    overflow: hidden;
    background: #f5f5f5;
    height: 100rpx;
    display: flex;
    align-items: center;
    padding:0 32rpx;
    font-size: 32rpx;
}
.sify {
    box-sizing: border-box;
    height: 112rpx;
    padding-bottom: 18rpx;
    overflow: hidden;
}
.sify .no-data{
    width: 100%;
    height: 112rpx;
    background: #fcfcfc;
    color: #fcfcfc;
}
.sify .no-data .scroll-view_H{
    color: #fcfcfc;
}

.scroll-view_H {
    display: flex;
    height: 66rpx;
    padding: 0rpx 28rpx 0rpx 12rpx;
    margin-top: 34rpx;
    margin-bottom: 12rpx;
    white-space: nowrap;
    justify-content: center;
    align-items: center;
    box-sizing: border-box;
}

.sify .scroll-view_H .scroll-view-item_H {
    display: inline-block;
    padding: 0 25rpx;
    height: 60rpx;
    text-align: center;
    font-size: 28rpx;
    font-weight: 500;
}

.sify .scroll-view_H .scroll-view-item_H.active {
    font-size: 28rpx;
    font-weight: bold;
    color: rgba(51, 51, 51, 1);
}
.sify .scroll-view_H .scroll-view-item_H.active .line {
    width: 40rpx;
    height: 8rpx;
    margin: 0 auto;
    margin-top: 10rpx;
    background: #ff5156;
}

.search-filter {
    padding: 30rpx 0rpx 0rpx 20rpx;
    box-sizing: border-box;
    font-size: 30rpx;
    color: #666;
    height: 88rpx;
    background-color: white;
}

.search-filter .item-container {
    display: flex;
    align-items: center;
    flex-wrap: nowrap;
}

.search-filter .item {
    width: 120rpx;
    margin-right: 30rpx;
    height: 62rpx;
    line-height: 62rpx;
    display: inline-block;
}

.search-filter .item.active {
    color: #ff5353;
    border-bottom: 2px solid #ff5353;
}

.filter-item {
    width: 92%;
    margin: 0 auto;
    border-bottom: none;
    background-color: white;
}
.fix-padding {
    border-bottom: 1rpx solid rgba(153, 153, 153, 0.6);
}
.filter-item .item {
    width: 20%;
    height: 50rpx;
    margin-bottom: 30rpx;
    font-size: 28rpx;
    line-height: 50rpx;
    display: inline-block;
    text-align: center;
    color: #666666;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.filter-item .item.active {
    color: #ff5353;
}
.answer {
    width: 92%;
    height: auto;
    padding-top: 20rpx;
    margin: 0 auto;
    background: #ffffff;
    box-sizing: border-box;
    border-bottom: 1rpx solid #f3f3f3;
    overflow: hidden;
}
.answer .an-head {
    height: 80rpx;
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.an-head .h-left {
    display: flex;
    align-items: center;
    overflow: hidden;
}
.an-head .h-left .l-avar {
    width: 60rpx;
    height: 60rpx;
}
.an-head .h-left .l-name {
    max-width: 50%;
    margin-left: 20rpx;
    font-size: 28rpx;
    font-weight: bold;
    color: rgba(102, 102, 102, 1);
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.an-head .h-right {
    max-width: 40%;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-wrap: nowrap;
    white-space: nowrap;
    padding: 0 25rpx;
    height: 50rpx;
    background: rgba(255, 227, 227, 1);
    border-radius: 25rpx;
    font-size: 24rpx;
    font-weight: 500;
    color: rgba(255, 95, 97, 1);
}
.an-con {
    width: 100%;
    height: auto;
    font-size: 34rpx;
    line-height: 54rpx;
    font-weight: 500;
    color: rgba(51, 51, 51, 1);
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}
.an-info {
    width: 100%;
    height: 80rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 24rpx;
    font-weight: 500;
    color: rgba(153, 153, 153, 1);
}
.goTop {
    height: 100rpx;
    width: 100rpx;
    box-sizing: border-box;
    padding: 10rpx 0;
    font-size: 24rpx;
    position: fixed;
    bottom: 120rpx;
    background: rgba(255, 255, 255, 0.8);
    right: 30rpx;
    border-radius: 50%;
    color: #999;
    border: 1px solid #ddd;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    align-items: center;
}
.g-img {
    width: 21rpx;
    height: 14rpx;
    background: url(data:image/png;base64,)
        center no-repeat;
    background-size: 100%;
}
/* 主体 */
.l-main {
    overflow-x: hidden;
    overflow-y: auto;
    box-sizing: border-box;
    overflow: hidden;
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    margin-bottom: 100rpx;
}
.item-container{
    white-space : nowrap;
}
.info-img{
    display: block;
    width: 487rpx;
    height: 285rpx;
    margin:0 auto;
    margin-top:100rpx;
}