/* pages/livedetail/livedetail.wxss */

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.livedetail-top {
    height: 50rpx;
    background: #f3f3f3;
    line-height: 70rpx;
}
.location{
    color: #999999;
    font-weight: bold;
    font-size: 26rpx;
    padding:34rpx;
}
.change{
    width:166rpx;
    text-align: center;
    height: 40rpx;
    line-height: 40rpx;
    position: absolute;
    right: 34rpx;
    top:15rpx;
    border:1px solid #FF5353;
    color: #FF5353;
    border-radius: 20rpx;
    font-size: 22rpx;
}
.l-mian {
    overflow-x: hidden;
    overflow-y: auto;
    box-sizing: border-box;
    overflow: hidden;
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
}

.contain {
    border-top: 20rpx solid #f3f3f3;
    border-bottom: 20rpx solid #f3f3f3;
    padding: 0 30rpx;
    width: 100%;
    box-sizing: border-box;
    overflow: hidden;
}

.nobottom {
    border-bottom: 0;
}

.c-name {
    /* height: 80rpx; */
    line-height: 80rpx;
    font-size: 34rpx;
    font-family: PingFang SC;
    font-weight: bold;
    color: rgba(53, 53, 53, 1);
    display: flex;
    /* align-items: center; */
}

.c-name image {
    width: 32rpx;
    height: 30rpx;
    display: block;
    margin-right: 18rpx;
    float: left;
    margin-top: 26rpx;
}

.c-name .n-con {
    float: right;
    width: 90%;
}

.c-one {
    display: flex;
    font-size: 28rpx;
    color: #353535;
    justify-content: space-between;
    align-items: center;
    height: 72rpx;
}

.c-one text {
    color: #666;
}

.c-one .c-line {
    width: 48%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-break: break-all;
    word-wrap: break-word;
}

.c-xia {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-break: break-all;
    word-wrap: break-word;
}

/* 公司 */

.detail {
    display: flex;
    justify-content: space-around;
    align-items: center;
    padding: 38rpx 30rpx;
}

.d-logo {
    display: block;
    width: 200rpx;
    height: 100rpx;
    border-radius: 8rpx;
}

.detail .d-xq {
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 58%;
}

.d-xq .x-title {
    font-size: 30rpx;
    font-weight: bold;
    color: rgba(51, 51, 51, 1);
    line-height: 44rpx;
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-break: break-all;
    word-wrap: break-word;
}

.d-xq .x-des {
    font-size: 26rpx;
    font-weight: 400;
    color: rgba(153, 153, 153, 1);
    line-height: 44rpx;
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-break: break-all;
    word-wrap: break-word;
}

/* nav */

.sify {
    box-sizing: border-box;
    border-bottom: 1rpx solid #d5d5d5;
    border-top: 0rpx;
}

.scroll-view_H {
    display: flex;
    height: 80rpx;
    line-height: 80rpx;
    white-space: nowrap;
    justify-content: center;
    align-items: center;
    box-sizing: border-box;
}

.sify .scroll-view_H .scroll-view-item_H {
    display: inline-block;
    min-width: 80rpx;
    margin: 0 30rpx 0 10rpx;
    height: 100%;
    text-align: center;
    font-size: 30rpx;
    font-family: PingFang-SC-Medium;
    font-weight: 500;
    color: rgba(0, 0, 0, 1);
}

.sify .scroll-view_H .scroll-view-item_H.active {
    color: #ff5353;
    font-weight: bold;
    border-bottom: 4rpx solid #FF5353;
    box-sizing: border-box;
}

/* 詳情 */

.goods {
    padding: 0 30rpx;
    margin-bottom: 100rpx;
    width: 100%;
    box-sizing: border-box;
    overflow: hidden;
}

.goods .g-item {
    border-bottom: 1rpx solid #e5e5e5;
}

.g-top {
    display: flex;
    height: 80rpx;
    width: 100%;
    justify-content: space-between;
    align-items: center;
    margin-top: 20rpx;
}

.g-top .t-left {
    width: 48%;
    display: flex;
    align-items: center;
    font-size: 32rpx;
    font-weight: bold;
    color: rgba(51, 51, 51, 1);
    overflow: hidden;
}

.g-top .t-left image {
    display: block;
    width: 80rpx;
    height: 40rpx;
    margin-right: 18rpx;
}

.g-top .t-left .name {
    width: 60%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-break: break-all;
    word-wrap: break-word;
}

.g-top .t-right {
    text-align: right;
    font-size: 28rpx;
    font-weight: 500;
    color: rgba(53, 53, 53, 1);
}

.g-top .t-right .time {
    color: #888;
}
.g-top .t-right .xiao-face{
    width: 40rpx;
    height: 40rpx;
    float: left;
}
.xiao-face image{
    width:100%;
    height: 100%;
}
.t-right .text{
    font-size:24rpx;
    font-weight:500;
    color:#333;
    float: left;
    margin:5rpx 0 0 10rpx;
}
.g-center{
    height:39rpx;
    font-size:24rpx;
    font-weight:bold;
    color:#333;
    line-height:39rpx;
    margin:0 0 10rpx 87rpx;
}
.g-bottom .des {
    font-size: 28rpx;
    font-weight: 500;
    color: rgba(102, 102, 102, 1);
    line-height: 42rpx;
    margin-bottom: 18rpx;
    margin-left: 98rpx;
}
.g-bottom .tu {
    display: flex;
    margin-bottom: 20rpx;
    justify-content: space-between;
    align-content: space-between;
    flex-wrap: wrap;
    overflow: hidden;
    margin-left: 98rpx;
}

.g-bottom .tu image {
    border-radius: 6rpx;
}

.g-bottom .tu .tu1 {
    width: 100%;
    overflow: hidden;
    height:400rpx;
}

.g-bottom .tu .tu2 {
    width: 48%;
    height: 334rpx;
    margin-bottom: 20rpx;
}

.g-bottom .tu .tu3 {
    width: 32%;
    height: 220rpx;
    margin-bottom: 12rpx;
}
.g-bottom .time-dianzan {
    display: flex;
    height:39rpx;
    line-height:39rpx;
    margin:0 0 10rpx 98rpx;
}
.g-bottom .time-dianzan .time{
    flex: 1;
    font-size:24rpx;
    font-weight:500;
    color:#999;
    
}
.g-bottom .time-dianzan  .dianzan{
    flex: 1;
    
}
.g-bottom .time-dianzan  .dianzan .zan{
    width:26rpx;
    height: 26rpx;
    line-height: 26rpx;
    float: right;
}
.g-bottom .time-dianzan  .dianzan .zan image{
    width: 100%;
    height: 100%;
    vertical-align: middle;
}
.g-bottom .time-dianzan  .dianzan  .zan-num{
    font-size:28rpx;
    font-weight:500;
    color:#999;
    float:right;
    margin-left: 10rpx;
}
.g-bottom .yanshou-dianping{
    margin-left: 98rpx;
    display: flex;
    margin-bottom: 20rpx;
}
.g-bottom .yanshou-dianping .yanshou{
    width: 47%;
    height:72rpx;
    line-height: 72rpx;
    border:2rpx solid #F5F5F5;
    border-radius:8rpx;
    margin-right: 15rpx;
}
.g-bottom .yanshou-dianping .dianping{
    width: 47%;
    height:72rpx;
    line-height: 72rpx;
    border:2rpx solid #F5F5F5;
    border-radius:8rpx;
}
.laugh-face{
    width:40rpx;
    height: 40rpx;
    line-height: 67rpx;
    float: left;
    padding-left: 15%;
}
.laugh-face image{
    width:100%;
    height: 100%;
    vertical-align: middle;
}
.jieduan{
    font-size:28rpx;
    font-weight:bold;
    color:#576B95;
    float: left;
    margin-left: 7rpx;
}
.tu .clear {
    height: 0rpx !important;
    margin-bottom: 0rpx !important;
}

/* 固定底部 */

.nodata {
    width: 100%;
    height: 560rpx;
    font-size: 24rpx;
    font-weight: 500;
    color: rgba(102, 102, 102, 1);
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
}

.nodata image {
    width: 376rpx;
    height: 210rpx;
}

.btm-nomore {
    display: block;
    width: 100%;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 24rpx;
    color: #999;
    text-align: center;
    margin-bottom: 200rpx;
}

.page-group-position {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1;
    background: #fff;
}

.swiper-wrap {
    height: 100%;
    width: 100%;
    text-align: center;
}

.swiper-mask {
    position: fixed;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.7);
    left: 0px;
    top: 0px;
    z-index: 4;
}

.swan-swiper-item {
    display: table;
}

.swiper-item image {
    position: absolute;
    margin: auto;
    top: 0px;
    bottom: 0px;
    left: 0px;
    right: 0px;
}

.num-box {
    position: fixed;
    z-index: 5;
    width: 100%;
    text-align: center;
    bottom: 30rpx;
    left: 0px;
    color: #fff;
}
.live-tit{
  width:60%;
  height: 70rpx;
  line-height: 70rpx;
  position: absolute;
  top:0;
  bottom: 0;
  right: 0;
  left:0;
  margin:auto;
  background: rgba(0,0,0,0.6);
  border-radius: 10rpx;
  font-size:28rpx;
  color:#fff;
  text-align: center;
  z-index:999;
}
/*更换装修地址*/
.change-address-mask{
    width:100%;
    height:100%;
    position: fixed;
    top:0;
    background: rgba(0,0,0,0.5);
    z-index:111;
}
.address-content{
    width:65%;
    height: 492rpx;
    background: #fff;
    border-radius: 16rpx;
    position: absolute;
    top:0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    padding:0 30rpx;
}
.address-content view{
    font-size:26rpx;
    text-align: center;
}
.address-content .address-tit{
    font-size: 32rpx;
    font-weight: bold;
    margin-top: 30rpx;
    margin-bottom: 10rpx;
    text-align: center;
}
.address-order{
    margin-top:40rpx;
    position:relative;
}
.address-order .input_style{
    padding-left:17rpx;
    height:60rpx;
    background: #F7F7F7;
    border-radius: 8rpx;
    margin-bottom: 30rpx;
    text-align: left;
    line-height: 60rpx;
}
.input_style input{
    height: 100%;
    width:62%;
}
.address-order .input_style text{
    width:160rpx;
    height: 40rpx;
    border:1px #FF5353 solid;
    background:#FFEDED;
    color:#FF5353;
    position: absolute;
    right: 12rpx;
    bottom: 8rpx;
    font-weight: bold;
    border-radius:  20rpx;
    font-size:22rpx;
    line-height: 40rpx;
    z-index:99;
    text-align: center;
}
.change-btn{
    height: 80rpx;
    background: #FF5353;
    color:#fff;
    font-weight: bold;
    font-size:26rpx;
    border-radius: 8rpx;
    line-height: 80rpx;
}
.closeBtn{
    display: inline-block;
    width:44rpx;
    height: 44rpx;
    position: absolute;
    right: 18rpx;
    top: 20rpx;
}
/* 点击评论 */
.pinglun{
    width:100%;
    height: 176rpx;
    background: #fff;
    padding: 25rpx;
    position: fixed;
    bottom: 0;
    left:0;
    right: 0;
}
textarea{
    width:70%;
    height:134rpx;
    background:#F7F7F7;
    border-radius:8rpx;
    padding: 16rpx 20rpx;
    float: left;
    font-size:24rpx;
}
.placeholder{
    font-size:24rpx;
    font-weight:500;
    color:#999;
    line-height:40rpx;
}
button.fb-btn{
    height:176rpx;
    font-size:32rpx;
    font-weight:500;
    color:#999;
    line-height:176rpx;
    background: #fff;
}
button::after {
  border: none;
}
/* 发布评论之后的弹窗 */
.mask{
    width:100%;
    height: 100%;
    background: rgba(0,0,0,.6);
    position: fixed;
    top:0;
    left:0;
}
.mask .inner{
    width:550rpx;
    height: 660rpx;
    background: #fff;
    border-radius:8px;
    position: absolute;
    top:0;
    left:0;
    right: 0;
    bottom: 0;
    margin:auto;
    z-index: 999
}
.mask .inner .inner-top{
    width:550rpx;
    height: 272rpx;
    background-size: cover;
    background-repeat:no-repeat;
    /* margin:auto; */
}
.mask .inner .inner-top .hdjl{
    width:210rpx;
    height:54rpx;
    margin: auto;
}
.mask .inner .inner-top .hdjl image{
    width:100%;
    height:100%;
    margin: 69rpx auto 24rpx;
}
.hdjl-box{
    padding-top: 90rpx;
}
.hdjl-text{
    font-size:32rpx;
    font-weight:500;
    color:#fff;
    text-align: center;
}
.ewm{
    width:200rpx;
    height: 200rpx;
    background: lightblue;
    margin: 30rpx auto;
}
.ewm image{
    width:100%;
    height:100%;
}
.ewm-text{
    font-size:28rpx;
    font-weight:500;
    color:#999;
    line-height:48rpx;
    text-align: center;
}
.close{
    width:60rpx;
    height:60rpx;
    border:2rpx solid rgba(255,255,255,1);
    border-radius:50%;
    font-size: 60rpx;
    color:#fff;
    line-height: 52rpx;
    text-align: center;
    margin: 90rpx auto
}
/* 我的印象 */
.yinxiang{
    padding-left: 93rpx;
}
.me-yx{
    width:120rpx;
    font-size:24rpx;
    font-weight:bold;
    color:#333;
    display: inline-block;
}
.yx-box{
    width:400rpx;
    display: inline-block;
    vertical-align: top;
}
.yx-tag{
    padding: 5rpx 15rpx;
    height:40rpx;
    background:rgba(255,248,226,1);
    border-radius:20rpx;
    display: inline-block;
    font-size:24rpx;
    font-weight:500;
    color:rgba(255,171,63,1);
    text-align: center;
    line-height: 22px;
    margin-right: 20rpx;
    margin-bottom:20rpx;
}
/* 评论 */
.comment{
    width:600rpx;
    height: auto;
    background: #F5F5F5;
    margin: 0 0 20rpx 93rpx;
    padding: 20rpx;
    box-sizing: border-box;
}
.comment .me-comment{
    font-size:24rpx;
    font-weight:bold;
    line-height:39rpx;
}
.comment .me-comment .me{
    width:50rpx;
    color:rgba(87,107,149,1);
    display: inline-block;
    vertical-align: top;

}
.neirong{
    width: 500rpx;
    color:#333;
    display: inline-block;
    word-wrap:break-word;
 
}
.shangjia{
    width:120rpx;
    color:rgba(87,107,149,1);
    display: inline-block;
    vertical-align: top;
}
.shangjia-huifu{
    width:410rpx;
    color:#333;
    display: inline-block;
    word-wrap:break-word;
}
