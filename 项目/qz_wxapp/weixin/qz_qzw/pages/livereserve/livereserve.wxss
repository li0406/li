/* pages/livereserve/livereserve.wxss */
.live-bg{
  width:368rpx;
  margin:54rpx auto;
}
.live-bg image{
  width:100%;
  height: 342rpx;
}
.reserve-box{
  padding:0 70rpx;
}
.reserve-tit{
  margin-bottom: 75rpx;
}
.xiaoqi_logo{
  width:85rpx;
  height:151rpx;
  float:left;
  margin-right:32rpx;
}
.xiaoqi_logo image{
  width:100%;
  height: 100%;
}
.reserve-success{
  font-size:30rpx;
  font-weight: bold;
  color: #666666;
  height: 120rpx;
  padding-top:31rpx;
}
.live-design{
  width:100%;
  height: 68rpx;
  margin-top:138rpx;
}
.live-design image{
  width:100%;
  height: 100%;
}
.change-address{
  text-align: center;
  color:#666666;
  font-size:24rpx;
  margin-top:120rpx;
}
.reserve-btn{
  height: 80rpx;
  margin:0 70rpx;
  text-align: center;
  line-height: 80rpx;
  background: #FF5353;
  border-radius: 40rpx;
  font-weight: bold;
  color: #fff;
  margin-top:80rpx;
}
.shengming{
  margin:0 10rpx 0 70rpx;
  display: initial;
}
.shengming image{
  width:20rpx;
  height: 20rpx;
}
.shengming-text{
  font-size:24rpx;
  color:#999;
}
.live-tit{
  width:80%;
  height: 70rpx;
  line-height: 70rpx;
  position: absolute;
  top:0;
  bottom: 0;
  right: 0;
  left:0;
  margin:auto;
  background: rgba(0,0,0,0.6);
  border-radius: 10rpx;
  font-size:28rpx;
  color:#fff;
  text-align: center;
  z-index:999;
}
/*更换装修地址*/
.change-address-mask{
    width:100%;
    height:100%;
    position: fixed;
    top:0;
    background: rgba(0,0,0,0.5);
    z-index:111;
}
.address-content{
    width:65%;
    height: 492rpx;
    background: #fff;
    border-radius: 16rpx;
    position: absolute;
    top:0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    padding:0 30rpx;
}
.address-content view{
    font-size:26rpx;
    text-align: center;
}
.address-content .address-tit{
    font-size: 32rpx;
    font-weight: bold;
    margin-top: 30rpx;
    margin-bottom: 10rpx;
    text-align: center;
}
.address-order{
    margin-top:40rpx;
    position:relative;
}
.address-order .input_style{
    padding-left:17rpx;
    height:60rpx;
    background: #F7F7F7;
    border-radius: 8rpx;
    margin-bottom: 30rpx;
    text-align: left;
    line-height: 60rpx;
}
.input_style input{
    height: 100%;
    width:62%;
}
.address-order .input_style text{
    width:160rpx;
    height: 40rpx;
    border:1px #FF5353 solid;
    background:#FFEDED;
    color:#FF5353;
    position: absolute;
    right: 12rpx;
    bottom: 8rpx;
    font-weight: bold;
    border-radius:  20rpx;
    font-size:22rpx;
    line-height: 40rpx;
    z-index:99;
    text-align: center;
}
.change-btn{
    height: 80rpx;
    background: #FF5353;
    color:#fff;
    font-weight: bold;
    font-size:26rpx;
    border-radius: 8rpx;
    line-height: 80rpx;
}
.closeBtn{
    display: inline-block;
    width:44rpx;
    height: 44rpx;
    position: absolute;
    right: 18rpx;
    top: 20rpx;
}