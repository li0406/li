.tips {
  font-size: 24rpx;
  font-weight: bold;
  color: rgba(153, 153, 153, 1);
  line-height: 26rpx;
  padding-left: 46rpx;
  margin: 20rpx auto 50rpx;
}

.lines {
  width: 100%;
  height: 30rpx;
  background: #f8f8f8;
}

.head1 {
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  margin-top: 40rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
}

.head2 {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
}

.head3 .item {
  width: 50%;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 26rpx;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
}

.head3 .item image {
  width: 26rpx;
  height: 26rpx;
}

.head3 .item view {
  width: 210rpx;
  text-align: left;
  margin-left: 12rpx;
}

.head3 {
  display: flex;
  flex-wrap: wrap;
}

.code {
  display: flex;
  align-items: center;
  padding: 32rpx 80rpx;
  box-sizing: border-box;
  justify-content: center;
}

.code .qr {
  width: 134rpx;
  height: 134rpx;
  display: block;
  margin-right: 32rpx;
}

.qr-title {
  font-size: 28rpx;
  font-weight: bold;
  color: #333;
  line-height: 52rpx;
}

.qr-title text{
  color: #CC3300;
  margin: 0 14rpx;
}
