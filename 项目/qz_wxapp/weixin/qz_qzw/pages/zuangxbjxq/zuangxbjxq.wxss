}

.imgbox .cf {
  position: absolute;
  top: 20rpx;
  left: 462rpx;
  text-align: center;
}

.imgbox .qt {
  position: absolute;
  top: 151rpx;
  left: 64rpx;
  text-align: center;
}

.imgbox .kt {
  position: absolute;
  top: 125rpx;
  left: 592rpx;
  text-align: center;
}

.imgbox .ws {
  position: absolute;
  top: 286rpx;
  left: 136rpx;
  text-align: center;
}

.imgbox .sd {
  position: absolute;
