  height: 500rpx;
  font-size: 25rpx;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 42rpx;
}

.imgbox text {
  color: #ff3f51;
  font-weight: bold;
}

.imgbox image {
  position: absolute;
  width: 627rpx;
  height: 345rpx;
