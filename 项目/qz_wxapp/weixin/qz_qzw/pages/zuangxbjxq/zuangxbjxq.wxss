.resetjis {
  width: 690rpx;
  height: 90rpx;
  margin: 50rpx auto;
  background: rgba(240, 62, 72, 1);
  border-radius: 8rpx;
  font-size: 32rpx;
  font-weight: bold;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  line-height: 90rpx;
  margin-bottom: 100rpx;
}

.title {
  width: 100%;
  height: 150rpx;
  font-size: 36rpx;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  display: flex;
  justify-content: center;
  align-items: center;
}

.title text {
  font-size: 50rpx;
  font-weight: bold;
  color: rgba(255, 63, 81, 1);
}

.imgbox {
