/* pages/addReport/addReport.wxss */
.clearfix:after {
  content: "";
  display: block;
  clear: both;
}
page {
  background:rgba(243,243,243,1);
  width:100%;
  box-sizing: border-box;
}
.style-choose {
  background: #fff;
  border-top: 1px solid #eee;
}
.style-choose .type {
  padding: 20rpx 20rpx 0 20rpx;
  font-size: 30rpx;
}
.style-choose .ul {
  height: 180rpx;
  padding: 20rpx;
}
.style-choose .ul .li {
  float: left;
  padding: 20rpx 40rpx;
  width: 210rpx;
  box-sizing: border-box;
  background: rgba(243,243,243,1);
  font-size: 24rpx;
  margin-right: 40rpx;
  margin-bottom: 20rpx;
  border-radius: 10rpx;
  text-align: center;
  color: #333;
}
.style-choose .ul .li:nth-child(3n) {
  margin-right: 0;
}
.info {
  background: #fff;
  margin-top: 20rpx;
  padding-left: 30rpx;
}
.delay .info{
  background: #fff;
  margin-top: 20rpx;
  padding: 0 30rpx;
}
.info.no-margin {
  margin-top: 0;
  border-top: 1px solid #eee;
}
.info .li {
  min-height: 84rpx;
  line-height: 84rpx;
  font-size: 28rpx;
  color: #333;
  border-bottom: 1px solid #e5e5e5;
  position: relative;
}
.info .li.pb30 {
  padding-bottom: 30rpx;
}
.info .li.no-border {
  border-bottom: none;
}
.info .li input {
  width: 60%;
  float: left;
  text-align: right;
  height: 84rpx;
  line-height:84rpx;
}
.info .li.company-name .fr {
  word-break: break-all;
  display: block;
  width: 67%;
  line-height: normal;
  padding-top: 20rpx;
  text-align: right;
}
.info .li.delay-company-name .fr {
  word-break: break-all;
  display: block;
  width: 73%;
  line-height: normal;
  padding-top: 20rpx;
  text-align: right;
}
.info .li.contact-name input {
  width: 79%;
}
.info .li.company-name input {
  width: 75%;
}

.info .li.contact-job input {
  width: 84%;
}
.info .li.tel input,.info .li.three-name input,.info .li.city input {
  width: 83%;
}
.info .li.phone input {
  width: 79%;
}
.info .li.wechat{
  border-bottom: 0;
}
.info .li.wechat input {
  width: 84%;
}
.info .li.zong-money input {
  width: 68%
}
.info .li.get-money input {
  width: 64%
}
.info .li.yu-money input {
  width: 73%
}
.info .li.logo-wz input {
  width: 70%
}
.info .li.tonglan-wz input {
  width: 73%
}
.info .li.tonglan-zs input {
  width: 63.5%
}
.info .li.order-cn input {
  width: 66%
}
.info .li.wd-code input, .info .li.ad input {
  width: 55%;
}
.info .li.tonglan-zs input {
  width: 59%;
}
.info .li.pl20 {
  padding-left: 20rpx;
}
.info .li.pl40 {
  padding-left: 40rpx;
}
.info .li text {
  float: left;
}
.info .li view.stime{
  text-align: right;
  padding-right: 60rpx;
}
.info .li .fr60 {
  float: right;
  margin-right: 60rpx;
}
.info .li text.red {
  color: red;
  float: none;
}
.info .li text.green-yq {
  color: green;
}
.info .li text.red-yq {
  color: red;
}
.info .li text.c90f {
  color:#90f;
}
.info .li text.cf00 {
  color:#f00;
  margin-left: 5rpx;
}
.info .li text.fr {
  float: right;
  margin-right: 60rpx;
}
.delay .info text.fr {
  float: right;
  margin-right: 0;
}
.delay .info .delay-icon image{
  float: right;
  width: 40rpx;
  height: 40rpx;
  margin-top: 20rpx;
  margin-right: 10rpx;
}
.info .li text.f-r {
  float: right;
}
.info .li text.mr30 {
  margin-right: 30rpx;
}
.info .li text.c-name {
  width: 530rpx;
  text-align: right;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.info .li.company-jc {
  padding-left: 40rpx;
}
.info .city .select-text {
  text-align: right;
  padding-right: 60rpx;
}
.info .li textarea {
  width:650rpx;
  height:260rpx;
  line-height: 40rpx;
  background:rgba(243,243,243,1);
  border-radius:10rpx;
  padding: 15rpx 20rpx;
  margin-bottom: 20rpx;
}
.info .li textarea.h130 {
  height:130rpx;
}
.info .li .wd-yz {
  width: 120rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  float: right;
  font-size: 12px;
  margin-right: 20rpx;
  padding: 0;
  margin-top: 8rpx;
}
.bottom {
  padding: 20rpx 30rpx;
  background: #fff;
}
.bottom .btns {
  height: 100rpx;
}
.bottom .btns view {
  width: 320rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  font-size: 32rpx;
  border: 2px solid #ff3b3b;
  float: left;
  border-radius: 10rpx;
}
.bottom .btns view.save {
  background: #fff;
  color: #ff3b3b;
}
.bottom .btns view.submit {
  background: #ff3b3b;
  color: #fff;
  margin-right: 0;
  float: right;
}
.bottom .tips {
  color: #ff3b3b;
  font-size: 24rpx;
}
.uploader-img {
  min-height: 100rpx;
}
.uploader-list {
  width: 200rpx;
  height: 200rpx;
  float: left;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  position: relative;
}
.uploader-list .item {
  display: block;
  width: 200rpx;
  height: 200rpx;
}
/* add+ */
.smallBb{
  color: #5DBB16;
}
.smallBbact{
  color: #FF3202;
}
.dianpu{
  width: 40rpx;
  height: 40rpx;
  float: right;
  margin-top: 20rpx;
  margin-right: 10rpx;
}
.modal-mask{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    z-index: 1000;
    opacity: 0.5;
}
 
.modal-dialog{
    width: 540rpx;
    height: 310rpx;
    position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    margin: auto;
    z-index: 1001;
    background-color: #fff;
    border-radius: 36rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}
.modal-dialog .m-title{
  font-size:30rpx;
  font-weight:bold;
  color:rgba(51,51,51,1);
  height: 128rpx;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.modal-dialog .m-con{
  width: 460rpx;
  height: 70rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size:28rpx;
  font-weight:bold;
  color:rgba(102,102,102,1);
}
.modal-dialog .m-con .m-nav{
  color: #1890FF;
}
.modal-dialog .m-error{
  position: absolute;
  top:-24rpx;
  right:-24rpx;
  width: 48rpx;
  height: 48rpx;
  z-index: 1 
}