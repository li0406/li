/* pages/hisxReport/hisxReport.wxss */

page {
  background: rgba(243, 243, 243, 1);
  width: 100%;
}

.list {
  background: rgba(243, 243, 243, 1);
  width: 100%;
  overflow: hidden;
}

.list .ul {
  background: rgba(243, 243, 243, 1);
  padding-top: 20rpx;
}

.list .ul .li {
  background: #fff;
  margin-bottom: 20rpx;
  padding: 0 30rpx;
  position: relative;
  font-size: 28rpx;
}

.list .ul .li:last-child {
  margin-bottom: 200rpx;
}

.list .ul .li .status {
  position: absolute;
  top: 40rpx;
  right: 0;
  height: 48rpx;
  line-height: 48rpx;
  padding-left: 10rpx;
  padding-right: 10rpx;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  border-radius: 24rpx;
}

.list .ul .li .wait-sub {
  background: #1890ff;
}

.list .ul .li .wait-examine {
  background: #ff6700;
}

.list .ul .li .pass {
  background: #5dbb16;
}

.list .ul .li .no-pass {
  background: #ff3b3b;
}

.list .ul .li .company-name {
  font-size: 34rpx;
  height: 50rpx;
  color: #333;
  padding-top: 40rpx;
  position: relative;
}

.list .ul .li .company-name .jianchen {
  width: 74%;
  font-size: 34rpx;
  font-weight: bold;
  color: #333;
  margin-left: 0;
  float: left;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  display: flex;
  align-items: center;
}

.list .ul .li .company-name .jianchen .j-com {
  font-size: 34rpx;
  font-weight: bold;
  color: #333;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.list .ul .li .company-name .jianchen .j-name {
  font-size: 28rpx;
  color: #999;
}

.list .ul .li .company-name .quyu {
  max-width: 220rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  float: left;
  height: 40rpx;
  padding-top: 6rpx;
}

.list .ul .li .company-name text {
  font-size: 28rpx;
  color: #666;
  font-weight: normal;
  margin-left: 20rpx;
}

.list .ul .li .w50 {
  float: left;
  width: 50%;
  margin: 30rpx 0 0;
  color: #333;
}

.list .ul .li .w100 {
  float: left;
  width: 100%;
  margin: 30rpx 0 0;
  color: #333;
}

.list .ul .li .w50 .t2 {
  color: #666;
  word-break: break-all;
}
.list .ul .li .mr20 {
  margin-right: 20rpx;
}

.list .ul .li .w50 .c-name {
  display: inline-block;
  margin-right: 6rpx;
  width: 224rpx;
  overflow: hidden;
  white-space: nowrap;
}

.list .ul .li .w100 .citytext {
  float: left;
}

.list .ul .li .w100 .t2 {
  color: #666;
  word-break: break-all;
}

.list .ul .li .w100 .t2.cityname {
  float: left;
  width: 250rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.list .ul .li .w100 .fl {
  float: left;
}

.list .ul .li .w100 .address {
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  word-break: break-all;
}

.list .ul .li .log-report {
  margin: 10rpx -30rpx 0;
  display: flex;
}

.list .ul .li .log-report view {
  flex: 1;
  float: left;
  height: 80rpx;
  line-height: 80rpx;
  color: #333;
  text-align: center;
  position: relative;
  border-top: 1px solid #e5e5e5;
  border-right: 1px solid #e5e5e5;
}

.list .ul .li .log-report view.w50 {
  width: 374rpx;
}

.list .ul .li .log-report view:last-child {
  border-right: none;
}

.list .ul .li .log-report image {
  display: inline-block;
  width: 28rpx;
  height: 28rpx;
  position: relative;
  top: 4rpx;
  left: -10rpx;
}

.list .ul .li .log-report view.w50 image {
  left: 120rpx;
}

.list .ul .li .hkname {
  line-height: 48rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.list .ul .li .rname {
  width: 332rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.list .ul .li .info .city {
  position: relative;
}
.list .ul .li .info .imglist {
  padding: 20rpx 0;
  overflow:hidden;
}
.list .ul .li .info .imglist .l-img{
  width: 200rpx;
  height: 200rpx;
  float: left;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
}

.list .ul .li .ml20{
  margin-left: 20rpx;
}

.list .ul .li .w100 .payment-xu{
  color: #ffb018;
}

.no-result {
  margin-top: 100rpx;
}

.no-result image {
  display: block;
  width: 250rpx;
  height: 250rpx;
  margin: 0 auto;
}

.no-result view {
  text-align: center;
  color: #999;
  font-size: 28rpx;
  margin-top: 50rpx;
}

.no-result button {
  width: 270rpx;
  height: 70rpx;
  line-height: 70rpx;
  background: rgba(255, 59, 59, 1);
  border-radius: 8rpx;
  color: #fff;
  font-weight: bold;
  font-size: 28rpx;
  margin: 50rpx auto;
}
.methods .t1{
  width: 22%;
  float: left;
  line-height: 50rpx;
}
.methods .t2-con{
  width: 78%;
  float: left;
  line-height: 50rpx;
}