/* pages/fendetails/fendetails.wxss */
.fendetails-box{
  padding:0 30rpx 0 30rpx;
}
.fendetails-box image{
  width:26rpx;
  height: 26rpx;
  position: absolute;
  top:360rpx;
  left:56rpx;
}
.input-name{
  height:88rpx;
  background:rgba(255,255,255,1);
  box-shadow:0px 8rpx 40rpx 0px rgba(214,214,214,0.5);
  border-radius:44rpx;
  margin-top:330rpx;
  font-size:28rpx;
  padding-left:64rpx;
}
.search{
  width:400rpx;
  height:70rpx;
  text-align: center;
  line-height: 70rpx;
  margin:0 auto;
  background:rgba(255,59,59,1);
  border-radius:36rpx;
  margin-top:160rpx;
  color:#ffffff;
  font-size: 36rpx;
}
.fendan-tips{
  width:100%;
  height: 100%;
}
.tips-content{
  width:280rpx;
  height:280rpx;
  position: absolute;
  top:0;
  right: 0;
  left: 0;
  bottom: 0;
  margin:auto;
  background:#000000;
  opacity:0.6;
  border-radius:20rpx;
}
.tips-content image{
  display: block;
  margin:0 auto;
  width:72rpx;
  height:72rpx;
  margin-top:56rpx;
  margin-bottom:18rpx;
}
.tips-content text{
  display: block;
  text-align: center;
  font-size:30rpx;
  color:#FFFFFF;
}
.tips-content text.fail-reason{
  font-size:24rpx;
  margin-top:20rpx;
}
.noData{
  height:80rpx;
  position: absolute;
  top:430rpx;
  left:70rpx;
  font-size: 24rpx;
  color:red;
}