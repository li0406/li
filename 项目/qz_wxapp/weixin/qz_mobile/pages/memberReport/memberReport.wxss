/* pages/memberReport/memberReport.wxss */
page {
  background:rgba(243,243,243,1);
  width:100%;
}
.header {
  height: 70rpx;
  line-height: 60rpx;
  background: #fff;
  width:100%;
  overflow: hidden;
  position: fixed;
  top:0;
  z-index: 999;
}
.header  .dingwei {
  float: left;
  margin-left: 20rpx;
  margin-right: 20rpx;
  font-weight: bold;
  font-size: 28rpx;
}
.header .search {
  width:476rpx;
  height:60rpx;
  line-height: 60rpx;
  background:rgba(243,243,243,1);
  border-radius:30rpx;
  float: left;
  position: relative;
}
.header .search .icon {
  width: 28rpx;
  height: 28rpx;
  position: absolute;
  top: 16rpx;
  left: 16rpx;
}
.header .search .icon image {
  display: block;
  width: 100%;
  height: 100%;
}
.header .search input {
  padding-left: 55rpx;
  font-size: 28rpx; 
  height:60rpx;
  line-height: 60rpx;
}
.header .select {
  float: right;
  margin-right: 20rpx;
  height: 70rpx;
  line-height: 70rpx; 
}
.header .select image {
  display: block;
  float: left;
  width: 50rpx;
  height: 50rpx;
  margin-top: 10rpx;
}
.header .select view {
  float: left;
  font-size: 28rpx;
  margin-left: 10rpx;
}
/* tabÊ†è */

.tab-back {
  background: #fff;
  width: 100%;
  overflow: hidden;
}

.tab {
  width: 92%;
  height: 66rpx;
  display: flex;
  margin: 0 auto;
  margin-top: 70rpx;
  justify-content: space-between;
  align-items: center;
  background: #fff;
}

.tab .flex1 {
  width: 33.33%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
}

.flex_text {
  max-width: 178rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-right: 8rpx;
}

.tab .active image {
  transform: rotate(180deg);
  background: none;
}

.tab image {
  width: 22rpx;
  height: 22rpx;
}

.list {
  background:rgba(243,243,243,1);
  width:100%;
  overflow: hidden;
}
.list .ul {
  background:rgba(243,243,243,1);
  padding-top: 20rpx; 
}
.list .ul .li {
  background: #fff;
  margin-bottom: 20rpx;
  /* height: 462rpx; */
  padding: 0 30rpx;
  position: relative;
  font-size: 28rpx;
}
.list .ul .li .status {
  position: absolute;
  top: 10rpx;
  right: 0;
  height: 48rpx;
  line-height: 48rpx;
  padding-left: 10rpx;
  padding-right: 10rpx; 
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  border-radius:24rpx;
}
.list .ul .li .wait-sub {
  background:#1890ff;
}
.list .ul .li .wait-examine {
  background: #78beff;
}
.list .ul .li .pass {
  background: #5dbb16;
}
.list .ul .li .no-pass {
  background: #ff3b3b;
}
.list .ul .li .wait-upload {
  background: #acde87;
}
.list .ul .li .success-upload {
  background: #5dbb16;
}
.list .ul .li .no-pass-examine {
  background: #ff3b3b;
}

.list .ul .li .company-name {
  font-size: 34rpx;
  height: 50rpx;
  color: #333;
  padding-top: 20rpx; 
}
.list .ul .li .company-name .jianchen{
  width: 100%; 
  font-size:34rpx;
  font-weight:bold;
  color:#333;
  margin-left:0;
  float: left;
  overflow:hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.list .ul .li .company-name .quyu {
  max-width:220rpx;
  overflow:hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  float: left;
  height: 40rpx;
  padding-top: 6rpx;
}
.list .ul .li .company-name text {
  font-size: 28rpx;
  color: #666;
  font-weight: normal;
  margin-left: 30rpx;
}
.list .ul .li .w50 {
  float: left;
  width: 50%;
  margin: 10rpx 0;
  color: #333;
}
.list .ul .li .w100 {
  float: left;
  width: 100%;
  margin: 10rpx 0;
  color: #333;
}
.list .ul .li .w50 .t2 {
  color: #666;
  word-break: break-all;
}
.list .ul .li .w50 .c-name {
  display: inline-block;
  float: right;
  margin-right: 6rpx;
  width: 224rpx;
  overflow: hidden;
  white-space: nowrap;
}
.list .ul .li .w100 .citytext {
  float: left;
}
.list .ul .li .w100 .t2 {
  color: #666;
  word-break: break-all;
}
.list .ul .li .w100 .t2.cityname {
  float: left;
  width: 250rpx;
  overflow:hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.list .ul .li .w100 .fl{
  float: left;
}
.list .ul .li .w100 .address {
  display:-webkit-box;
  -webkit-line-clamp:2;
   -webkit-box-orient: vertical;
   overflow: hidden;
   word-break: break-all;
}
.list .ul .li .info {
  position: relative;
}
.list .ul .li .log-report {
  margin: 10rpx -30rpx 0;
  display: flex;
}

.list .ul .li .log-report view {
  flex: 1;
  float: left;
  height: 80rpx;
  line-height: 80rpx;
  color: #333;
  text-align: center;
  position: relative;
  border-top: 1px solid #E5E5E5;
  border-right: 1px solid #E5E5E5;
}
.list .ul .li .log-report view.w50 {
  width: 374rpx;
}
.list .ul .li .log-report view:last-child {
  border-right: none;
}
.list .ul .li .log-report image {
  display: inline-block;
  width: 28rpx;
  height: 28rpx;
  position: relative;
  top: 4rpx;
  left: -10rpx;
}
.list .ul .li .log-report view.w50 image {
  left: 120rpx;
}

.page {
  width: 100%;
  height:60rpx;
  padding-left: 124rpx;
  margin: 60rpx 0;
}
.page .pre, .page .next {
  float: left;
  width:134rpx;
  height:60rpx;
  line-height: 60rpx;
  text-align: center;
  color: #fff;
  font-size: 24rpx;
  background:rgba(255,59,59,1);
  border:2rpx solid rgba(255,59,59,1);
  border-radius:6rpx;
}
.page .disabled {
  background: #ccc;
  border-color: #ccc;
}
.page .total {
  float: left;
  width:134rpx;
  height:60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 28rpx;
  color: #353535;
  background:rgba(255,255,255,1);
  border-radius:6rpx;
  margin: 0 50rpx;
}
.no-result {
  margin-top: 100rpx;
}
.no-result image {
  display: block;
  width: 250rpx;
  height: 250rpx;
  margin: 0 auto;
}
.no-result view {
  text-align: center;
  color: #999;
  font-size: 28rpx;
  margin-top: 50rpx;
}
.no-result button {
  width:270rpx;
  height:70rpx;
  line-height: 70rpx;
  background:rgba(255,59,59,1);
  border-radius:8rpx;
  color: #fff;
  font-weight: bold;
  font-size: 28rpx;
  margin: 50rpx auto;
}