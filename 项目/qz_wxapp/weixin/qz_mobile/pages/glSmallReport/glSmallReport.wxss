page {
  background: rgba(243, 243, 243, 1);
  width: 100%;
}

.header {
  height: 70rpx;
  line-height: 60rpx;
  background: #fff;
  width: 100%;
  overflow: hidden;
  position: fixed;
  top: 0;
  z-index: 999;
}

.header .dingwei {
  float: left;
  margin-left: 20rpx;
  margin-right: 20rpx;
  font-weight: bold;
  font-size: 28rpx;
}

.header .search {
  width: 590rpx;
  height: 60rpx;
  line-height: 60rpx;
  background: rgba(243, 243, 243, 1);
  border-radius: 30rpx;
  float: left;
  position: relative;
}

.header .search .icon {
  width: 28rpx;
  height: 28rpx;
  position: absolute;
  top: 16rpx;
  left: 16rpx;
}

.header .search .icon image {
  display: block;
  width: 100%;
  height: 100%;
}

.header .search input {
  padding-left: 55rpx;
  font-size: 28rpx;
  height: 60rpx;
  line-height: 60rpx;
  padding-right: 20rpx;
}

.header .select {
  float: right;
  margin-right: 20rpx;
  height: 70rpx;
  line-height: 70rpx;
}

.header .select image {
  display: block;
  float: left;
  width: 50rpx;
  height: 50rpx;
  margin-top: 10rpx;
}

.header .select view {
  float: left;
  font-size: 28rpx;
  margin-left: 10rpx;
}

/* tab栏 */

.tab-back {
  background: #fff;
  width: 100%;
  overflow: hidden;
}

.tab {
  width: 92%;
  height: 66rpx;
  display: flex;
  margin: 0 auto;
  margin-top: 70rpx;
  justify-content: space-between;
  align-items: center;
  background: #fff;
}

.tab .flex1 {
  width: 33.33%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
}

.flex_text {
  max-width: 178rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-right: 8rpx;
}

.tab .active image {
  transform: rotate(180deg);
  background: none;
}

.tab image {
  width: 22rpx;
  height: 22rpx;
}

.list {
  background: rgba(243, 243, 243, 1);
  width: 100%;
  overflow: hidden;
  margin-top: 70rpx;
}

.list .ul {
  background: rgba(243, 243, 243, 1);
  padding-top: 20rpx;
}

.list .ul .li {
  background: #fff;
  margin-bottom: 20rpx;
  padding: 0 30rpx;
  padding-left: 0rpx;
  font-size: 28rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.list .ul .li .lable {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.list .ul .li .li-left {
  margin-left: 20rpx;
  width: 80rpx;
  height: 100%;
  display: inline-block;
  align-items: center;
}

/*复选框外框样式*/
checkbox .wx-checkbox-input {
  width: 28rpx;
  height: 28rpx;
  border: 2rpx solid #E5E5E5;
  border-radius: 100%;
}

/*复选框外框选中样式*/
checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  border-color: #FF3B3B;
}

/*复选框选中后内部样式*/
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  width: 20rpx;
  height: 20rpx;
  background: #FF3B3B;
  border-radius: 100%;
  content: '';
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}

.li-left .l-aniu {
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
}

.list .ul .li .li-right {
  width: 630rpx;
  float: right;
}

.list .ul .li .status {
  position: absolute;
  top: 4rpx;
  right: 0;
  height: 48rpx;
  line-height: 48rpx;
  padding: 0 16rpx;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  border-radius: 24rpx;
}

.list .ul .li .wait-sub {
  background: #1890ff;
}

.list .ul .li .wait-examine {
  background: #ff6700;
}

.list .ul .li .pass {
