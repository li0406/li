.page {
  width: 100%;
  height: 60rpx;
  padding-left: 124rpx;
  margin: 60rpx 0;
}

.page-more {
  margin-bottom: 160rpx;
}

.page .pre,
.page .next {
  float: left;
  width: 134rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  color: #fff;
  font-size: 24rpx;
  background: rgba(255, 59, 59, 1);
  border: 2rpx solid rgba(255, 59, 59, 1);
  border-radius: 6rpx;
}

.page .disabled {
  background: #ccc;
  border-color: #ccc;
}

.page .total {
  float: left;
  width: 134rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 28rpx;
  color: #353535;
  background: rgba(255, 255, 255, 1);
  border-radius: 6rpx;
  margin: 0 50rpx;
}

.no-result {
  margin-top: 100rpx;
}

.no-result image {
  display: block;
  width: 250rpx;
  height: 250rpx;
  margin: 0 auto;
}

.no-result view {
  text-align: center;
  color: #999;
  font-size: 28rpx;
  margin-top: 50rpx;
}

.no-result button {
  width: 270rpx;
  height: 70rpx;
  line-height: 70rpx;
  background: rgba(255, 59, 59, 1);
  border-radius: 8rpx;
  color: #fff;
  font-weight: bold;
  font-size: 28rpx;
  margin: 50rpx auto;
}

/* 弹框 */


.glmodel {
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  position: fixed;
  bottom: 0;
  left: 0;
  background: #fff;
  z-index: 999;
  box-shadow: 0px 0px 7px 0px rgba(0, 0, 0, 0.2);
}

.glmodel .btn {
  width: 50%;
  height: 100%;
  font-size: 28rpx;
  font-weight: bold;
  color: rgba(24, 144, 255, 1);
  display: flex;
  align-items: center;
  justify-content: center;
}

.glmodel .canel {
  box-sizing: border-box;
  border-right: 1rpx solid #e5e5e5;
  color: #666;
}


/* 弹框 */

.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  z-index: 1000;
  opacity: 0.5;
}

.modalDlg {
  width: 580rpx;
  height: 350rpx;
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  margin: auto;
  z-index: 1001;
  background-color: #fff;
  border-radius: 36rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.modalDlg .title {
  font-size: 30rpx;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  margin: 40rpx;
}

.modalDlg .foot {
  position: absolute;
  bottom: 0;
  left: 0;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-top: 1rpx solid #e6e6e6;
  width: 100%;
}

.modalDlg .foot .item {
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  font-weight: 500;
  color: #1890ff;
  width: 50%;
  height: 100%;
}

.modalDlg .foot .bit {
  color: #999;
  border-right: 1rpx solid #e6e6e6;
}

.modalDlg .m-con {
  font-size: 24rpx;
  font-weight: 500;
  color: rgba(255, 59, 59, 1);
  width: 80%;
  line-height: 40rpx;
}