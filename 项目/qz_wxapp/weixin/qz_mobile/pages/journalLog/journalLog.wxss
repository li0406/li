/* pages/journalLog/journalLog.wxss */
page {
  background: #f3f3f3;
}
.add {
  position: fixed;
  bottom: 20rpx;
  right: 20rpx;
}
.add image {
  display: block;
  width: 95rpx;
  height: 95rpx;
}
.ul {
  margin-top: 20rpx;
  color: #333;
  padding-bottom: 20rpx;
}
.ul .li {
  padding: 40rpx 30rpx;
  background: #fff;
  margin-bottom: 20rpx;
}
.ul .li .title {
  margin-bottom: 28rpx;
}
.ul .li .title .name {
  font-size: 34rpx;
  font-weight: bold;
}
.ul .li .title .way {
  display: inline-block;
  width:84rpx;
  height:36rpx;
  line-height: 36rpx;
  font-size: 24rpx;
  color: #fff;
  text-align: center;
  border-radius:18rpx;
  margin-left: 18rpx;
}
.ul .li .title .green {
  background:rgba(93,187,22,1);
}
.ul .li .title .red {
  background:rgba(255,59,59,1);
}
.ul .li .title .blue {
  background:rgba(24,144,255,1);
}
.ul .li .title .orange {
  background:rgba(255,137,59,1);
}
.ul .li .title .num {
  float: right;
  font-size: 28rpx;
}
.ul .li .info {
  color: #333;
  font-size: 28rpx;
}
.ul .li .info .t2 {
  color: #666;
  word-break: break-all;
}
.ul .li .info .time {
  margin-bottom: 27rpx;
}
.page {
  width: 100%;
  height:60rpx;
  padding-left: 124rpx;
  margin: 60rpx 0;
}
.page .pre, .page .next {
  float: left;
  width:134rpx;
  height:60rpx;
  line-height: 60rpx;
  text-align: center;
  color: #fff;
  font-size: 24rpx;
  background:rgba(255,59,59,1);
  border:2rpx solid rgba(255,59,59,1);
  border-radius:6rpx;
}
.page .total {
  float: left;
  width:134rpx;
  height:60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 28rpx;
  color: #353535;
  background:rgba(255,255,255,1);
  border-radius:6rpx;
  margin: 0 50rpx;
}
.page .disabled {
  background: #ccc;
  border-color: #ccc;
}