.btn{
  width: 50%;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  font-size: 32rpx;
  background-color: #fff;
  color: #333333;
  flex-grow:1;
  border-top: 2rpx solid #e5e5e5;
}
.btn image{
  display: inline-block;
  width: 28rpx;
  height: 28rpx;
  margin-right: 10rpx;
  vertical-align: middle;
}
.history-btn{
  background-color: #fff;
  border-right: 2rpx solid #e5e5e5;
}
.check-box{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 90rpx;
  z-index: 999;
  display: flex;
  justify-content: space-between;
}
.offer-btn{
  position: fixed;
  left: 64rpx;
  bottom:200rpx;
  width: 110rpx;
  height: 110rpx;
}
.offer-btn image{
    width: 100%;
    height: 100%;
}
.mask-offer{
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background-color: rgba(0,0,0,0.4);
  z-index: 9999;
}
.close-area{
  left: 0;
  top:0;
  height: 100%;
  width: 30%;
  float: left;
}
.offer-ul{
  right: -70%;
  top:0;
  height: 100%;
  width: 70%;
  font-size: 30rpx;
  background-color: white;
  float: right;
  position: relative;
  overflow-y: auto;
  transition: all 0.3s;
}
.offer-ul.active{
  right: 0;
}
.offer-li{
  height: 70rpx;
  line-height: 70rpx;
  padding-left: 10rpx;
  border-bottom: 1rpx solid #e5e5e5;
}

.offer-ul .table {
  position: absolute;
  top: 0rpx;
  left: auto;
  right: 0;
  margin: auto;
  width: 100%;
  font-size: 14px;
  padding-bottom: 200rpx;
}
.offer-ul .table .mytr{
  text-align: center;
  position: relative;
}
.offer-ul .table .mytr .one{
  display:inline-block;
  width:30%;
}
.offer-ul .table .mytr .one .one-key{
  border: 0.5px solid #ECECEC;
  border-bottom: none;
  border-right: none;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  width: 30%;
}
.absolute-height-center{
  position: absolute;
  left:50%;
  top:50%;
  transform: translate(-50%, -50%)
}
.offer-ul .table .mytr .two-block{
  display:inline-block;
  width:70%;
  
}
.offer-ul .table .mytr .two, .offer-ul .table .mytr .three{
  display:inline-block;
  width:50%;
}
 
.offer-ul .tr {
  display: flex;
  width: 100%;
  border-top: 1rpx solid #555;
  border-left: 1rpx solid #555;
  color: #555;
  box-sizing: border-box;
}
.offer-ul .tr_title{
  display:block;
  text-align:center;
  padding:20rpx 0;
  border-right: 1px solid #333;
}
.offer-ul .td {
  width: 40%;
  justify-content: center;
  text-align: center;
  border-right: 1rpx solid #555;
  padding: 20rpx 0;
}
 
.offer-ul .th {
  justify-content: center;
  display: flex;
  height: 3rem;
  align-items: center;
  color: #555;
  border-right: 1rpx solid #555;
}
 
.offer-ul .td_center {
  display: flex;
  align-items: center;
}
 
.offer-ul .tdd2 {
  border-bottom: 1px solid #333;
  padding: 4px;
}
 
.offer-ul .width1 {
  width: 40%;
}
 
.offer-ul .width2 {
  width: 30%;
}
 
.offer-ul .bg-g {
  background: #e6f3f9;
}
 
.offer-ul .border-none {
  border: none;
}

.check-body{
  font-size: 32rpx;
  padding:33rpx;
}
.radio-part{
  padding-left: 90rpx;
  margin-bottom: 20rpx;
}
.note-part-text{
  float: left;
  width: 90rpx;
}
.note-part-textarea{
  overflow: hidden;
  background-color: #e5e5e5;
  border-radius: 0.3em;
  height: 400rpx;
}
.note-part-textarea textarea{
  background-color: #f3f3f3;
  width: 100%;
  height:400rpx;
}

.uploader-img {
  min-height: 100rpx;
}
.uploader-list {
  width: 200rpx;
  height: 200rpx;
  float: left;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  position: relative;
}
.uploader-list .item {
  display: block;
  width: 200rpx;
  height: 200rpx;
}