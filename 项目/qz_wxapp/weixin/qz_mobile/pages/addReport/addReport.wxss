/* pages/addReport/addReport.wxss */
page {
  background:rgba(243,243,243,1);
  width:100%;
  box-sizing: border-box;
}
.clearfix:after {
  content: "";
  display: block;
  clear: both;
}
.style-choose {
  background: #fff;
  border-top: 1px solid #eee;
}
.style-choose .type {
  padding: 20rpx 20rpx 0 20rpx;
  font-size: 30rpx;
}
.style-choose .ul {
  height: 349rpx;
  padding: 20rpx;
}
.style-choose .ul .li {
  float: left;
  padding: 20rpx 0;
  width: 210rpx;
  box-sizing: border-box;
  background: rgba(243,243,243,1);
  font-size: 24rpx;
  margin-right: 40rpx;
  margin-bottom: 20rpx;
  border-radius: 10rpx;
  text-align: center;
  color: #333;
  white-space: nowrap;
}
.style-choose .ul .li:nth-child(3n) {
  margin-right: 0;
}
.red{color: #ff3b3b;}
.info {
  background: #fff;
  margin-top: 20rpx;
  padding-left: 30rpx;
}
.info.no-margin {
  margin-top: 0;
  border-top: 1px solid #eee;
}
.info .li {
  min-height: 84rpx;
  line-height: 84rpx;
  font-size: 28rpx;
  color: #333;
  border-bottom: 1px solid #e5e5e5;
  position: relative;
}
.info .li.pb30 {
  padding-bottom: 30rpx;
}
.info .li.no-border {
  border-bottom: none;
}
.info .li input {
  width: 60%;
  float: left;
  text-align: right;
  height: 84rpx;
  line-height:84rpx;
}
.info .li input.w70{
  width: 70%;
}
.info .li input.w73{
  width: 73%;
}
.info .li input.other-order-desc{
    width: 100%;
    box-sizing: border-box;
    padding-right: 35rpx;
}
.info .li.contact-name input {
  width: 79%;
}
.info .li.company-name input {
  width: 75%;
}
.info .li.promised_orders input {
  width: 67%;
}
.info .li.contact-job input {
  width: 84%;
}
.info .li.tel input,.info .li.three-name input,.info .li.city input {
  width: 83%;
}
.info .li.phone input {
  width: 79%;
}
.info .li.wechat input {
  width: 84%;
}
.info .li.zong-money input {
  width: 68%
}
.info .li.fix-money input{
    width:76%;
}
.info .li.w59 input{
  width: 59%;
}
.info .li.w57 input{
  width: 57%;
}
.info .li.bc-bcz input{
  width:76%;
}
.info .li.fix-money .gift{
    width:78%;
}
.info .li.zxj-money input{
    width:80%;
}
.info .li.sever-money input{
    width:82%;
}
.info .li.get-money input {
  width: 64%
}
.info .li.yu-money input {
  width: 73%
}
.info .li.logo-wz input {
  width: 70%
}
.info .li.tonglan-wz input {
  width: 73%
}
.info .li.tonglan-zs input {
  width: 63.5%
}
.info .li.order-cn input {
  width: 70%
}
.info .li.wd-code input, .info .li.ad input {
  width: 55%;
}
.info .li.w95 input {
  width: 95%; 
}
.info .li.tonglan-zs input {
  width: 58%;
}
.info .li.pl20 {
  padding-left: 20rpx;
}
.info .li.pl40 {
  padding-left: 40rpx;
}
.info .li text {
  float: left;
}
.info .li .cs{
  float: right;
  margin-right: 60rpx;
}
.info .li view.stime{
  text-align: right;
  padding-right: 5%;
}
.info .li .fr60 {
  float: right;
  margin-right: 60rpx;
}
.info .li .fr40 {
  float: right;
  margin-right: 40rpx;
}
.info .li text.red {
  color: red;
  float: none;
}
.info .li .c6 {
  color:#666;
}
.info .li text.fr {
  float: right;
  margin-right: 60rpx;
}
.info .li text.mr30 {
  margin-right: 28rpx;
}
.info .li text.c-name {
  width: 530rpx;
  text-align: right;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.info .li.company-jc {
  padding-left: 40rpx;
}
.info .city .select-text {
  text-align: right;
  padding-right: 60rpx;
}
.info .arrow {
  display: block;
  width: 14rpx;
  height: 25rpx;
  position: absolute;
  top: 30rpx;
  right: 30rpx;
}
.info .li textarea {
  width:650rpx;
  height:260rpx;
  line-height: 40rpx;
  background:rgba(243,243,243,1);
  border-radius:10rpx;
  padding: 15rpx 20rpx;
  margin-bottom: 20rpx;
}
.info .li textarea.h130 {
  height: 130rpx;
}
.info .li .wd-yz {
  width: 120rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  float: right;
  font-size: 12px;
  margin-right: 20rpx;
  padding: 0;
  margin-top: 8rpx;
}

/* 14 15  标杆会员*/
.info .li-no-text .no-text{
  color: #999;
  float: right;
  margin-right: 30rpx;
}

/* 会员延期 */
.info .wd-search{
  overflow: hidden;
}
.info .li .delay-days{
  float: right;
  text-align: right;
  margin-right: 30rpx;
  color: #999;
}
.info .li .delay-days text{
  float: none;
}
.info .li .delay-days-img{
  width: 40rpx;
  height: 40rpx;
  float: left;
  margin-top: 20rpx;
  margin-right: 10rpx;
}

.info .wd-search .wd-btn{
  width: 120rpx;
  height: 60rpx;
  font-size: 28rpx;
  line-height: 60rpx;
  margin-top: 10rpx;
}

.bottom {
  padding: 20rpx 30rpx;
  background: #fff;
}
.bottom .btns {
  height: 100rpx;
}
.bottom .btns view {
  width: 320rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  font-size: 32rpx;
  border: 2px solid #ff3b3b;
  float: left;
  border-radius: 10rpx;
}
.bottom .btns view.save {
  background: #fff;
  color: #ff3b3b;
}
.bottom .btns view.submit {
  background: #ff3b3b;
  color: #fff;
  margin-right: 0;
  float: right;
}
.bottom .tips {
  color: #ff3b3b;
  font-size: 24rpx;
}
.uploader-img {
  min-height: 100rpx;
}
.uploader-list {
  width: 200rpx;
  height: 200rpx;
  float: left;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  position: relative;
}
.uploader-list .delete {
  position: absolute;
  top: 0;
  right: 0;
  display: block;
  width: 40rpx;
  height: 40rpx;
}
.uploader-list .item {
  display: block;
  width: 200rpx;
  height: 200rpx;
}
.upAdd {
  float: left;
  width: 200rpx;
  height: 200rpx;
  background: #eee;
  text-align: center;
  font-size: 80px;
  color: #fff;
  line-height: 178rpx;
}