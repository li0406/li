  text-align: center;
  font-size: 40rpx;
}

.xibao-canvas{
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: #000;
  z-index: 9999;
}



.open-seting-bg {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 99999;
}

.open-seting-bg .btn-openset {
  display: flex;
  justify-content: space-around;
  font-size: 30rpx;
  margin-top: 80rpx;
}

.open-seting-bg .set-title {
  font-size: 30rpx;
  margin-top: 40rpx;
  text-align: center;
}

.open-seting-bg .open-set-inner {
  width: 400rpx;
  height: 220rpx;
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  margin: auto;
  background: #ffffff;
  padding: 30rpx;
}

.open-seting-bg .btn-openset > view:nth-child(1) {
  color: #919191;
}

.open-seting-bg .button-style {
  width: 100%;
  height: 100%;
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 0 0;
  font-size: 30rpx;
  line-height: 0;
  color: red;
}

.open-seting-bg button::after {
  border: 0;
}
