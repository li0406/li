page {
  background:rgba(243,243,243,1);
  width:100%;
  height: 100%;
}
.header {
  height: 70rpx;
  line-height: 60rpx;
  background: #fff;
  width:100%;
  overflow: hidden;
  position: fixed;
  top:0;
  z-index: 999;
}
.header  .dingwei {
  float: left;
  margin-left: 20rpx;
  margin-right: 20rpx;
  font-weight: bold;
  font-size: 28rpx;
}
.header .search {
  width: 610rpx;
  height:60rpx;
  line-height: 60rpx;
  background:rgba(243,243,243,1);
  border-radius:30rpx;
  float: left;
  position: relative;
}
.header .search .icon {
  width: 28rpx;
  height: 28rpx;
  position: absolute;
  top: 16rpx;
  left: 16rpx;
}
.header .search .icon image {
  display: block;
  width: 100%;
  height: 100%;
}
.header .search input {
  padding-left: 55rpx;
  font-size: 28rpx;
  height:60rpx;
  line-height: 60rpx;
}
.header .select {
  float: right;
  margin-right: 20rpx;
  height: 70rpx;
  line-height: 70rpx;
}
.header .select image {
  display: block;
  float: left;
  width: 50rpx;
  height: 50rpx;
  margin-top: 10rpx;
}
.header .select view {
  float: left;
  font-size: 28rpx;
  margin-left: 10rpx;
}
/* tabÊ†è */

.tab-back {
  background: #fff;
  width: 100%;
  overflow: hidden;
}

.tab {
  width: 92%;
  height: 66rpx;
  display: flex;
  margin: 0 auto;
  margin-top: 70rpx;
  justify-content: space-between;
  align-items: center;
  background: #fff;
}

.tab .flex1 {
  width: 33.33%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
}

.flex_text {
  max-width: 198rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-right: 8rpx;
}

.tab .active image {
  transform: rotate(180deg);
  background: none;
}

.tab image {
  width: 22rpx;
  height: 22rpx;
}
.fixcswidth{
  max-width: 175rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: inline-block;
  white-space: nowrap;
  vertical-align: top;
}
.isSmallR{
  display:inline-block;
  margin-left: 10rpx;
  border: 2rpx solid red;
  border-radius: 10rpx;
  color: red;
}
.isSmallG{
  display:inline-block;
  margin-left: 10rpx;
  border: 2rpx solid green;
  border-radius: 10rpx;
  color: green;
}
.list {
  background:rgba(243,243,243,1);
  width:100%;
  overflow: hidden;
}
.list .ul {
  background:rgba(243,243,243,1);
  padding-top: 20rpx;
}
.list .ul .li {
  background: #fff;
  margin-bottom: 20rpx;
  /* height: 462rpx; */
  padding: 0 30rpx;
  position: relative;
  font-size: 28rpx;
}
.list .ul .li .status {
  position: absolute;
  top: 40rpx;
  right: 30rpx;
  width: 115rpx;
  height: 48rpx;
  line-height: 48rpx;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  border-radius:24rpx;
}
.list .ul .li .wait-sub {
  background:#1890ff;
}
.list .ul .li .wait-examine {
  background: #78beff;
}
.list .ul .li .pass {
  background: #5dbb16;
}
.list .ul .li .no-pass {
  background: #ff3b3b;
}
.list .ul .li .wait-upload {
  background: #acde87;
}
.list .ul .li .success-upload {
  background: #5dbb16;
}
.list .ul .li .no-pass-examine {
  background: #ff3b3b;
}

.list .ul .li .company-name {
  font-size: 34rpx;
  color: #333;
  padding-top: 40rpx;
}
.list .ul .li .company-name .jianchen{
  font-size:34rpx;
  font-weight:bold;
  color:#333;
  margin-left:0;
}
.list .ul .li .company-name .red{
  color: #f00;
}
.list .ul .li .company-name .quyu {
  max-width:220rpx;
  overflow:hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  float: left;
  height: 40rpx;
  padding-top: 6rpx;
}
.list .ul .li .company-name text {
  font-size: 28rpx;
  color: #666;
  font-weight: normal;
  margin-left: 20rpx;
}
.list .ul .li .w50 {
  float: left;
  width: 50%;
  margin: 30rpx 0 0;
  color: #333;
}
.list .ul .li .w100 {
  float: left;
  width: 100%;
  margin: 30rpx 0 0;
  color: #333;
}
.list .ul .li .w50 .t2 {
  color: #666;
}
.list .ul .li .w50 .c-name {
  display: inline-block;
  float: right;
  margin-right: 6rpx;
  width: 224rpx;
  overflow: hidden;
  white-space: nowrap;
}
.list .ul .li .w30{
  float: left;
  width: 30%;
  margin: 30rpx 0 0;
  color: #333;
}
.list .ul .li .w70{
  float: left;
  width: 70%;
  margin: 30rpx 0 0;
  color: #333;
}
.list .ul .li .w100 .t2 {
  color: #666;
}
.list .ul .li .w100 .fl{
  float: left;
}
.list .ul .li .w100 .address {
  display:-webkit-box;
  -webkit-line-clamp:2;
   -webkit-box-orient: vertical;
   overflow: hidden;
   word-break: break-all;
}
.list .ul .li .info {
  position: relative;
}
.list .ul .li .log-report {
  margin: 10rpx -30rpx 0;
  display: flex;
  text-align: center
}
.list .ul .li .log-report image {
  display: block;
  width: 28rpx;
  height: 28rpx;
}
.list .ul .li .log-report view {
  flex: 1;
  width: 49.8%;
  float: left;
  height: 80rpx;
  line-height: 80rpx;
  color: #333;
  text-align: center;
  position: relative;
  border-top: 1px solid #E5E5E5;
  border-right: 1px solid #E5E5E5;
}
.list .ul .li .log-report view:last-child{
    border-right: 0;
}
.list .ul .li .log-report view.w50 {
  width: 374rpx;
}
.list .ul .li .log-report view:last-child {
  border-right: none;
}
.list .ul .li .log-report .iconfont {
  display: inline-block;
  width: 28rpx;
  height: 28rpx;
  position: relative;
  top: 4rpx;
  left: -20rpx;
}
.list .ul .li .log-report view.w50 image {
  left: 120rpx;
}


.page {
  width: 100%;
  height: 180rpx;
  padding: 0 155rpx;
  box-sizing: border-box;
  background: #f3f3f3;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.page .pre, .page .next {
  width: 134rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  color: #fff;
  font-size: 24rpx;
  background: rgba(255, 59, 59, 1);
  border: 2rpx solid rgba(255, 59, 59, 1);
  border-radius: 6rpx;
}

.page .active {
  background: #f7f7f7;
  border: 2rpx solid #dedede;
  color: #acacac;
}

.page .total {
  width: 134rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 28rpx;
  color: #353535;
  background: rgba(255, 255, 255, 1);
  border-radius: 6rpx;
  margin: 0 50rpx;
}
.red{
  color: #ff5353
}

.check-body{
  font-size: 32rpx;
  padding:33rpx;
}
.radio-part{
  padding-left: 90rpx;
  margin-bottom: 20rpx;
}
.note-part-text{
  float: left;
  width: 90rpx;
}
.note-part-textarea{
  overflow: hidden;
  background-color: #e5e5e5;
  border-radius: 0.3em;
  height: 400rpx;
}
.note-part-textarea textarea{
  background-color: #f3f3f3;
  width: 100%;
  height: 400rpx;
}
.icon-aim{
  font-size: 38rpx;
}
.fix-radius{
  display: block;
  border-radius: 24rpx;
  color: white;
  padding:0 10rpx;
  max-width: 170rpx;
}
.no-result {
  margin-top: 100rpx;
}
.no-result image {
  display: block;
  width: 250rpx;
  height: 250rpx;
  margin: 0 auto;
}
.no-result view {
  text-align: center;
  color: #999;
  font-size: 28rpx;
  margin-top: 50rpx;
}
.no-result button {
  width:270rpx;
  height:70rpx;
  line-height: 70rpx;
  background:rgba(255,59,59,1);
  border-radius:8rpx;
  color: #fff;
  font-weight: bold;
  font-size: 28rpx;
  margin: 50rpx auto;
}
.reject-modal-tip{
  font-size: 30rpx;
  line-height: 1.5;
  margin-bottom: 30rpx;
}
