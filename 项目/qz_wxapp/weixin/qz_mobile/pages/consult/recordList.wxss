/* pages/consult/recordList.wxss */
page{
  background: #f3f3f3;
}
.list .li{
  background: #fff;
  font-size: 28rpx;
  margin-top: 20rpx;
  padding: 20rpx 30rpx;
}
.list .li .item{
  line-height: 50rpx;
}
.list .li .item .w50{
  display: inline-block;
  width: 50%;
}
/*分页 */
.page {
  width: 100%;
  height: 180rpx;
  padding: 0 155rpx;
  box-sizing: border-box;
  background: #f3f3f3;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.page .pre, .page .next {
  width: 134rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  color: #fff;
  font-size: 24rpx;
  background: rgba(255, 59, 59, 1);
  border: 2rpx solid rgba(255, 59, 59, 1);
  border-radius: 6rpx;
}

.page .active {
  background: #f7f7f7;
  border: 2rpx solid #dedede;
  color: #acacac;
}

.page .total {
  width: 134rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 28rpx;
  color: #353535;
  background: rgba(255, 255, 255, 1);
  border-radius: 6rpx;
  margin: 0 50rpx;
}